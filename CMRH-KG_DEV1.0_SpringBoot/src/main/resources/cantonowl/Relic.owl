    <!-- http://www.owl-ontologies.com/Relic.owl#英国领事馆旧址 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#英国领事馆旧址">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Relic.owl#旧址"/>
