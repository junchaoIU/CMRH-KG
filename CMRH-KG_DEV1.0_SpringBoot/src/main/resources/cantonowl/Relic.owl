    <!-- http://www.owl-ontologies.com/Relic.owl#起义门 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#起义门">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Relic.owl#旧址"/>
