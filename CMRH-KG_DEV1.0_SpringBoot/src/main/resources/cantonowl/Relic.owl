    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Relic.owl#上党战役纪念馆 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#上党战役纪念馆">
