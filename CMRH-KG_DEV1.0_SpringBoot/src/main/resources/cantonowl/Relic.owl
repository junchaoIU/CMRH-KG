    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#兵谏亭">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Relic.owl#旧址"/>
        <Event:相关事件 rdf:resource="http://www.owl-ontologies.com/Event.owl#西安事变"/>
