    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#黄花岗七十二烈士墓园">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Relic.owl#陵园墓群"/>
