    <!-- http://www.owl-ontologies.com/Relic.owl#八一南昌起义会议旧址 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Relic.owl#八一南昌起义会议旧址">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Relic.owl#旧址"/>
