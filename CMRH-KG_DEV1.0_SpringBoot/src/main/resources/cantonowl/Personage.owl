    <!-- http://www.owl-ontologies.com/Personage.owl#夏衍 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#夏衍">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
