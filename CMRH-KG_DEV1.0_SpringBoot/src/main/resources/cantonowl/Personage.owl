    <!-- http://www.owl-ontologies.com/Personage.owl#沈青 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#沈青">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
