    <!-- http://www.owl-ontologies.com/Personage.owl#倪映典 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#倪映典">
        <Event:相关事件 rdf:resource="http://www.owl-ontologies.com/Event.owl#庚戌广州新军武装起义"/>        <Event:相关事件 rdf:resource="http://www.owl-ontologies.com/Event.owl#广州新军起义"/>        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">领导广州新军起义</主要成就>
