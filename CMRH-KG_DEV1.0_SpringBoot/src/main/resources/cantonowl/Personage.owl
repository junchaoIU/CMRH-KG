    <!-- http://www.owl-ontologies.com/Personage.owl#李林 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#李林">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">参与创建雁北抗日根据地</主要成就>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">福建龙溪县</出生地>
