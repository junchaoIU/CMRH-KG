    <!-- http://www.owl-ontologies.com/Personage.owl#罗范群 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#罗范群">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">梅州市梅县区</出生地>
