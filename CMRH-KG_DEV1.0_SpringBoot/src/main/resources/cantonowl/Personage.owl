    <!-- http://www.owl-ontologies.com/Personage.owl#陈垣 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#陈垣">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <学生 rdf:resource="http://www.owl-ontologies.com/Personage.owl#启功"/>
