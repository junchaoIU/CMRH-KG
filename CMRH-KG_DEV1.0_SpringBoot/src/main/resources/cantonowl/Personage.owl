    <!-- http://www.owl-ontologies.com/Personage.owl#马思聪 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#马思聪">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
