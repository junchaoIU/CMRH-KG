    <!-- http://www.owl-ontologies.com/Personage.owl#李达 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#李达">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <妻子 rdf:resource="http://www.owl-ontologies.com/Personage.owl#张乃一"/>
        <战友 rdf:resource="http://www.owl-ontologies.com/Personage.owl#唐天际"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1955年被授予上将军衔</主要成就>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">陕西眉县</出生地>
        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1905年4月19日</出生日期>
