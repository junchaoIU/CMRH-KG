    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#郁文">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <丈夫 rdf:resource="http://www.owl-ontologies.com/Personage.owl#乔石"/>
