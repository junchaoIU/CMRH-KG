    <!-- http://www.owl-ontologies.com/Personage.owl#蔡申熙 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#蔡申熙">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">湖南省醴陵县</出生地>
        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1906年</出生日期>
        <别名 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">蔡升熙，字旭初</别名>
        <毕业院校 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">黄埔军校第一期</毕业院校>
        <职业 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">军人</职业>
        <逝世日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1932年10月9日</逝世日期>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">蔡申熙(1906—1932)，是中国红军高级指挥员，军事家，黄埔军校第一期毕业生曾任国民革命军营长、团长，起义部队师参谋长，红15军军长、4军10师师长、中共鄂豫皖军委副主席、彭杨军校校长、第25军军长等职。其革命生涯虽短暂但轰轰烈烈，成为了革命事业中永恒的丰碑。蔡申熙是红15军的主要创始人。</rdfs:comment>
    </owl:NamedIndividual>
    


