    <!-- http://www.owl-ontologies.com/Personage.owl#朱克靖 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#朱克靖">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">湖南省醴陵县北乡株树下村朱家老屋</出生地>
