    <!-- http://www.owl-ontologies.com/Personage.owl#冯云山 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#冯云山">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">金田起义，全州战役</主要成就>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">广东花县禾落地村</出生地>
