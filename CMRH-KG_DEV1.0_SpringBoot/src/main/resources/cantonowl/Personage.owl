        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1888年</出生日期>
        <毕业院校 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">四川陆军速成学校</毕业院校>
        <职业 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">革命家</职业>
