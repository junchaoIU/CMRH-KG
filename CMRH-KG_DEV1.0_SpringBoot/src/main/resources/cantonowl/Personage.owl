    <!-- http://www.owl-ontologies.com/Personage.owl#黄名芊 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#黄名芊">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
