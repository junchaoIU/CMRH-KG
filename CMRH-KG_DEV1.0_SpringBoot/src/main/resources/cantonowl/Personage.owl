    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#张云逸">
        <Event:相关事件 rdf:resource="http://www.owl-ontologies.com/Event.owl#百色起义"/>        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
