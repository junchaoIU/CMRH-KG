    <!-- http://www.owl-ontologies.com/Personage.owl#瞿秋白 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#瞿秋白">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <前妻 rdf:resource="http://www.owl-ontologies.com/Personage.owl#王剑虹"/>
