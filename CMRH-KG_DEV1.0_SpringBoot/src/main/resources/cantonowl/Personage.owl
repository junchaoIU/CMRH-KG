        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1890年</出生日期>
        <毕业院校 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">广西陆军干部学堂</毕业院校>
        <职业 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">军人</职业>
