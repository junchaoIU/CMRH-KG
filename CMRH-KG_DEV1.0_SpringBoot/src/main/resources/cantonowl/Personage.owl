        <姐姐 rdf:resource="http://www.owl-ontologies.com/Personage.owl#宋霭龄"/>
        <弟弟 rdf:resource="http://www.owl-ontologies.com/Personage.owl#宋子安"/>
        <弟弟 rdf:resource="http://www.owl-ontologies.com/Personage.owl#宋子文"/>
        <弟弟 rdf:resource="http://www.owl-ontologies.com/Personage.owl#宋子良"/>
