    <!-- http://www.owl-ontologies.com/Personage.owl#梁广 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#梁广">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
