    <!-- http://www.owl-ontologies.com/Personage.owl#周之友 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#周之友">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
