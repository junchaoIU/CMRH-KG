    <!-- http://www.owl-ontologies.com/Personage.owl#曾友仁 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#曾友仁">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#无党派人物"/>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">广东五华</出生地>
        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1892</出生日期>
