    <!-- http://www.owl-ontologies.com/Personage.owl#程子华 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#程子华">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <战友 rdf:resource="http://www.owl-ontologies.com/Personage.owl#陈奇涵"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">原全国政协副主席中央顾问委员会常委</主要成就>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">山西省运城市解州镇</出生地>
        <出生日期 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1905年6月20日</出生日期>
        <毕业院校 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">黄埔军校</毕业院校>
