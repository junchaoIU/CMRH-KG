    <!-- http://www.owl-ontologies.com/Personage.owl#陈俊生 -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Personage.owl#陈俊生">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Personage.owl#共产党人物"/>
        <主要成就 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">原国务委员</主要成就>
        <出生地 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">黑龙江省桦南县</出生地>
